# 服务配置
NODE_ENV=development
PORT=3000

# 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_NAME=domain_config
DB_USER=root
DB_PASSWORD=password
DB_POOL_MIN=2
DB_POOL_MAX=10

# Redis 配置
REDIS_ENABLED=true
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_TTL=3600

# 日志配置
LOG_LEVEL=info
LOG_FILE=logs/app.log

# API 配置
API_PREFIX=/api/v1
MAX_PAGE_SIZE=100
DEFAULT_PAGE_SIZE=20

# 限流配置
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=100

# 管理界面配置
ADMIN_PASSWORD=admin123
